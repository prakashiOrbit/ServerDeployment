{
  "division": {
    "name": "Farmer Header",
    "formelements": {
      "title": "GENERAL DETAILS ",
      "section": 2,
      "generalDetails": [
        {
          "id": "fid",
          "label": "fid",
          "control": "textbox",
          "validation": "alphanumeric",
          "disabled": true
        },
        {
          "id": "name",
          "label": "name",
          "control": "textbox",
          "validation": "alphanumeric"
        },
        {
          "id": "phoneNo",
          "label": "phoneNo",
          "control": "textbox",
          "validation": "phone"
        },
        {
          "id": "Aadhar No",
          "label": "aadhar_no",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "label": "pan",
          "id": "PAN",
          "control": "textbox",
          "validate": "alphanumeric"
        }
      ],
      "title2": "ADDRESS DETAILS",
      "addressDetails": [
        {
          "id": "Street Address",
          "label": "street",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "City",
          "label": "city",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "State",
          "label": "state",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "Pin Code",
          "label": "pin",
          "control": "textbox",
          "validate": "number"
        }
      ],

      "title1": "PAYMENT DETAILS",
      "paymentDetails": [
        {
          "id": "Payee Name",
          "label": "payee",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "Account Number",
          "label": "account",
          "control": "textbox",
          "validate": "number"
        },
        {
          "id": "IFSC  Code",
          "label": "ifsc",
          "control": "textbox",
          "validate": "number"
        }
      ]
    },
    "buttons": [
      {
        "id": "create",
        "label": "Create",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "CreateFarmer"
      },
      {
        "id": "cancel",
        "label": "Cancel",
        "control": "button",
        "redirect": "/polist"
      }
    ],
    "edittable": [
      {
        "id": "abx",
        "label": "Farmer Data",
        "dataurl": "/Service/tabledata.json"
