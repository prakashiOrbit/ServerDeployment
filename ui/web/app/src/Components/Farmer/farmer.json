{
  "division": {
    "name": "Farmer Header",
    "formelements": [{
      "title": "GENERAL DETAILS ",
      "details": [
   
        {
          "id": "name",
          "label": "Name",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[^0-9]+$",
          "errorMessage":"The name contains only letters(eg. G. Prakash Murthy)"
        },
        {
          "id": "phoneNo",
          "label": "Phone Number",
          "control": "textbox",
          "required":true,
          "pattern": "^(\\d{3}|\\d{4})?(\\d{7}|\\d{8}|\\d{9}|\\d{10})$",
          "errorMessage":"Please enter valid contact number(eg. 9876567654, 04712314440)"
        },
        {
          "id": "aadharNo",
          "label": "Aadhar Number",
          "control": "textbox",
          "required":true,
          "pattern": "^[2-9]{1}[0-9]{3}[0-9]{4}[0-9]{4}$",
          "errorMessage":"There must be 12 digits in the Aadhar Number(eg. 765487598324)."
        },
        {
          "label": "PAN Number",
          "id": "panNo",
          "control": "textbox",
          "required":false
        }
      ]},
      {
      "title": "ADDRESS DETAILS",
      "details": [
        {
          "id": "village",
          "label": "Village",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[a-zA-Z0-9,#][a-zA-Z0-9,#\\s]*$",
          "errorMessage":"Input a valid village (eg.Antapur , Boral)."
        },
        {
          "id": "panchayat",
          "label": "Panchayat",
          "control": "textbox",
          "required":false
        },
        {
          "id": "taluk",
          "label": "Taluk",
          "control": "textbox",
          "required":false
        },
        {
          "id": "district",
          "label": "District",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[^0-9]+$",
          "errorMessage":"Input a valid District.(eg. Bagalkot, Bidar)"
        }
      ]},
{
      "title": "PAYMENT DETAILS",
      "details": [
        {
          "id": "payeeName",
          "label": "Payee Name",
          "control": "textbox",
          "required":true,
          "minLength":3,
          "pattern": "^[^0-9]+$",
          "errorMessage":"The Payee name only consists of letters.(Manish Malhotra)"
        },
        {
          "id": "accountNo",
          "label": "Account Number",
          "control": "textbox",
          "required":true,
          "minLength":9,
          "pattern": "^[0-9]{9,18}$",
          "errorMessage":"Enter the correct Account Number (eg. 876543256789)."
        },
        {
          "id": "bankNameAddress",
          "label": "Bank Name with Address",
          "control": "textbox",
          "required":false
        },
        {
          "id": "ifscCode",
          "label": "IFSC  Code",
          "control": "textbox",
          "required":true,
          "minLength":8,
          "pattern": "^[A-Z]{4}0[A-Z0-9]{6}$",
          "errorMessage":"The IFSC code must be valid."
        }
      ]
    }]
  }
}