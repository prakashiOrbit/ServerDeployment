{
  "division": {
    "name": "Farmer Header",
    "formelements": [{
      "title": "GENERAL DETAILS ",
      "details": [
        {
          "id": "fId",
          "label": "fId",
          "control": "textbox",
          "validation": "alphanumeric",
          "disabled": true,
          "required": false,
          "minLength": 3,
          "pattern": "^[A-Za-z0-9_-]*$",
          "errorMessage":
            "Username must be at least 3 characters long and contain only letters and numbers."
        },
        {
          "id": "name",
          "label": "name",
          "control": "textbox",
          "required":true,
          "pattern": "^[^0-9]+$",
          "errorMessage":"Username only contains letters(eg. G. Shivani Murthy)"
        },
        {
          "id": "phoneNo",
          "label": "phoneNo",
          "control": "textbox",
          "required":true,
          "pattern": "^[6-9][0-9]{9}$",
          "errorMessage":"Phone Number only contains 10 digits(eg. 9876567654)"
        },
        {
          "id": "Aadhar No",
          "label": "aadharNo",
          "control": "textbox",
          "required":true,
          "pattern": "^[2-9]{4} [0-9]{4} [0-9]{4}$",
          "errorMessage":"Aadhar Number must contains 12 digits.(eg. 7654 8759 8324)"
        },
        {
          "label": "panNo",
          "id": "pan",
          "control": "textbox",
          "required":true,
          "pattern": "^[A-Z]{5}[0-9]{4}[A-Z]{1}$",
          "errorMessage":"Please enter valid Pan Number.(eg. CQAYM7601W)"
        }
      ]},
      {
      "title": "ADDRESS DETAILS",
      "details": [
        {
          "id": "Street Address",
          "label": "streetAddress",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[a-zA-Z0-9][a-zA-Z0-9\\s]*$",
          "errorMessage":"Please enter valid Street Address.(eg. 13th Church Street)"
        },
        {
          "id": "City",
          "label": "city",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[a-zA-Z ]+$",
          "errorMessage":"City Name only contains letters.(Bangalore)"
        },
        {
          "id": "State",
          "label": "state",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "pattern": "^[a-zA-Z ]+$",
          "errorMessage":"State only contains letters.(eg. Tamil Nadu)"
        },
        {
          "id": "Pin Code",
          "label": "pinCode",
          "control": "textbox",
          "required":true,
          "minLength":6,
          "pattern": "^[1-9][0-9]{5}$",
          "errorMessage":"Pin Code only contains 6 digits.(EG. 495006)"
        }
      ]},
{
      "title": "PAYMENT DETAILS",
      "details": [
        {
          "id": "Payee Name",
          "label": "payeeName",
          "control": "textbox",
          "required":true,
          "minLength":3,
          "pattern": "^[^0-9]+$",
          "errorMessage":"Payee Name only contains letters.(Manish Malhotra)"
        },
        {
          "id": "Account Number",
          "label": "accountNo",
          "control": "textbox",
          "required":true,
          "minLength":9,
          "pattern": "^[0-9]{9,18}$",
          "errorMessage":"Please enter valid Account Number.(eg. 876543256789)"
        },
        {
          "id": "IFSC  Code",
          "label": "ifscCode",
          "control": "textbox",
          "required":true,
          "pattern": "^[A-Z]{4}0[A-Z0-9]{6}$",
          "errorMessage":"Please enter valid IFSC Code."
        }
      ]
    }],
    "buttons": [
      {
        "id": "create",
        "label": "Create",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "/farmerList"
      },
      {
        "id": "cancel",
        "label": "Cancel",
        "control": "button",
        "redirect": "/farmerList"
      }
    ]
  }
}
