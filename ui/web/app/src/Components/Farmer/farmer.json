{
  "division": {
    "name": "Farmer Header",
    "formelements": {
      "title": "GENERAL DETAILS ",
      "generalDetails": [
        {
          "id": "fId",
          "label": "fId",
          "control": "textbox",
          "validation": "alphanumeric",
          "disabled": true,
          "required": true,
          "minLength": 3,
          "pattern": "^[A-Za-z0-9_-]*$",
          "errorMessage":
            "Username must be at least 3 characters long and contain only letters and numbers."
        },
        {
          "id": "name",
          "label": "name",
          "control": "textbox",
          "validation": "alphanumeric",
          "required": true,
          "minLength": 3,
      
          "pattern": "^[A-Za-z0-9_-]*$",
          "errorMessage":
            "Username must be at least 3 characters long and contain only letters and numbers."
        },
        {
          "id": "phoneNo",
          "label": "phoneNo",
          "control": "textbox",
          "validation": "phone",
          "required": true,
          "minLength": 10,
          "pattern": "^[0-9]*$",
          "errorMessage":
            "Username must be at least 3 characters long and contain only letters ."
        },
        {
          "id": "Aadhar No",
          "label": "aadharNo",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "label": "panNo",
          "id": "pan",
          "control": "textbox",
          "validate": "alphanumeric"
        }
      ],
      "title2": "ADDRESS DETAILS",
      "addressDetails": [
        {
          "id": "Street Address",
          "label": "streetAddress",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "City",
          "label": "city",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "State",
          "label": "state",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "Pin Code",
          "label": "pinCode",
          "control": "textbox",
          "validate": "number"
        }
      ],

      "title1": "PAYMENT DETAILS",
      "paymentDetails": [
        {
          "id": "Payee Name",
          "label": "payeeName",
          "control": "textbox",
          "validate": "alphanumeric"
        },
        {
          "id": "Account Number",
          "label": "accountNo",
          "control": "textbox",
          "validate": "number"
        },
        {
          "id": "IFSC  Code",
          "label": "ifscCode",
          "control": "textbox",
          "validate": "number"
        }
      ]
    },
    "buttons": [
      {
        "id": "create",
        "label": "Create",
        "control": "button",
        "validationjs": "validatedata",
        "submitURL": "/farmerList"
      },
      {
        "id": "cancel",
        "label": "Cancel",
        "control": "button",
        "redirect": "/farmerList"
      }
    ]
  }
}
