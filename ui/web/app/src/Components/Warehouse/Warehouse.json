{
  "division": {
    "name": "Warehouse Header",
    "formelements": [{
      "title": "CREATE WAREHOUSE",
      "details": [
        {
          "label": "Warehouse Address",
          "id": "warehouseAddress",
          "control": "textbox",
          "required":true,
          "errorMessage":"Provide the warehouse address (eg. Trivandrum, Kerala)"
        },
        {
          "label": "Warehouse Name",
          "id": "warehouseName",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "errorMessage":"Enter a valid warehouse name (eg. F2R BLR)."
        },
        {
          "label": "Contact No",
          "id": "contactNo",
          "control": "textbox",
          "required":true,
          "pattern": "^(\\d{3}|\\d{4})?(\\d{7}|\\d{8}|\\d{9}|\\d{10})$",
          "errorMessage":"Contact number must be valid(eg. 9876567654, 04712314440)"
        }
      ]
    }]
  }
}
