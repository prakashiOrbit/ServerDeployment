{
  "division": {
    "name": "Truck",
    "formelements": {
      "title": "CREATE TRUCK AND DRIVER",
      "truckDetails": [
        {
          "label": "licenseNo",
          "id": "licenseNo",
          "control": "textbox",
          "required":"true",
          "pattern": "^[A-Z]{2}[0-9]{2}[0-9]{4}[0-9]{7}$",
          "errorMessage":"Enter valid License Number.(eg. CG1220237654634)"
        },
        {
          "id": "tranportCompanyName",
          "label": "tranportCompanyName",
          "control": "textbox",
          "required":true,
          "pattern": "^[^0-9]+$",
          "errorMessage":"Company Name only contains letters(eg. Manoj Transports)"
        },
        {
          "id": "VinNo",
          "label": "VinNo",
          "control": "textbox",
          "required":"true",
          "minLength":2,
          "pattern": "^[a-zA-Z0-9][a-zA-Z0-9\\s]*$",
          "errorMessage":"Please enter valid VIN Number.(eg. 4Y1SL65848Z411439)"
        },
        {
          "id": "companyName",
          "label": "companyName",
          "control": "textbox",
          "required":"true",
          "pattern": "^[^0-9]+$",
          "errorMessage":"Company Name only contains letters(eg. Manoj Transports)"
        },
        

        {
          "id": "contactNo",
          "label": "contactNo",
          "control": "textbox",
          "required":true,
          "pattern": "^[6-9][0-9]{9}$",
          "errorMessage":"Contact Number only contains 10 digits(eg. 9876567654)"
        },
        {
          "id": "address",
          "label": "address",
          "control": "textbox",
          "required":"true"
        }
      ]
    }
  }
}
