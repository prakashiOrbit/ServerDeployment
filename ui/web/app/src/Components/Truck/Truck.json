{
  "division": {
    "name": "Truck",
    "formelements": [{
      "title": "CREATE TRANSPORT",
      "details": [
        {
          "id": "tranportCompanyName",
          "label": "Tranport Company Name",
          "control": "textbox",
          "required":true,
          "errorMessage":"Enter a valid Company Name (eg. Manoj Transports)"
        },
        {
          "label": "License No",
          "id": "licenseNo",
          "control": "textbox",
          "required":true,
          "pattern": "^[A-Z]{2}[0-9]{2}[0-9]{4}[0-9]{7}$",
          "errorMessage":"License Number must be in a valid format(eg. CG1220237654634)."
        },

        {
          "id": "contactNo",
          "label": "Contact No",
          "control": "textbox",
          "required":true,
          "pattern": "^(\\d{3}|\\d{4})?(\\d{7}|\\d{8}|\\d{9}|\\d{10})$",
          "errorMessage":"Contact number must be valid(eg. 9876567654, 04712314440)"
        },
        {
          "id": "address",
          "label": "Address",
          "control": "textbox",
          "required":true,
          "minLength":2,
          "errorMessage":"Enter valid address details.(eg. 13th ,Church Street)"
        }
      ]
    }]
  }
}
